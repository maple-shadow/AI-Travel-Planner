# 应用配置
NODE_ENV=development
PORT=5000
FRONTEND_URL=http://localhost:3000
LOG_LEVEL=info

# 数据库配置
MONGODB_URI=mongodb://localhost:27017/ai-travel-planner

# JWT配置
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRES_IN=7d

# 百炼AI服务配置
BAILIAN_API_KEY=your_bailian_api_key
BAILIAN_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1

# 科大讯飞语音服务配置
IFLYTEK_APP_ID=your_iflytek_app_id
IFLYTEK_API_KEY=your_iflytek_api_key
IFLYTEK_API_SECRET=your_iflytek_api_secret

# AI服务配置
AI_RATE_LIMIT_PER_MINUTE=60
AI_RATE_LIMIT_PER_HOUR=1000
AI_TRIP_MODEL=qwen-turbo
AI_BUDGET_MODEL=qwen-turbo
AI_VOICE_MODEL=iflytek-speech

# 高德地图API配置
AMAP_API_KEY=your-amap-api-key

# Supabase配置
SUPABASE_URL=your-supabase-project-url
SUPABASE_ANON_KEY=your-supabase-anon-key

# 文件上传配置
UPLOAD_MAX_SIZE=10485760